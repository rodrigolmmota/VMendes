<h1>Listing profissionals</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Cpf</th>
    <th>Tel</th>
    <th>Cel</th>
    <th>Contato</th>
    <th>Tipo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profissionals.each do |profissional| %>
  <tr>
    <td><%= profissional.nome %></td>
    <td><%= profissional.cpf %></td>
    <td><%= profissional.tel %></td>
    <td><%= profissional.cel %></td>
    <td><%= profissional.contato %></td>
    <td><%= profissional.tipo %></td>
    <td><%= link_to 'Show', profissional %></td>
    <td><%= link_to 'Edit', edit_profissional_path(profissional) %></td>
    <td><%= link_to 'Destroy', profissional, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profissional', new_profissional_path %>
